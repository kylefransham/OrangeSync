<?xml version="1.0" encoding="UTF-8"?>
<sparkleshare>
  <plugin>
    <info>
        <_name>OrangeShare.Net</_name>
        <_description>The easiest way to share projects with OrangeShare</_description>
        <icon>ssnet.png</icon>
        <backend>Git</backend>
    </info>
    <address>
      <value>ssh://storage@sparkleshare.net/</value>
      <example></example>
    </address>
    <path>
      <value></value>
      <_example>/username/project</_example>
    </path>
  </plugin>
</sparkleshare>

